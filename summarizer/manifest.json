{
    "name": "Summarizer",
    "version": "0.1",
    "manifest_version": 3,
    "description": "An extension that saves your time by instantly summarizing youtube videos",
    "permissions": ["activeTab"],
    "host_permissions": ["http://127.0.0.1:5000/*"],
    "action":{
        "default_title": "Wanna summarize?",
        "default_popup":"popup.html"
    }
}